<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xmlns:context = "http://www.springframework.org/schema/context"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context.xsd">
<context:annotation-config/>
	<bean id="jobDetails" class="com.java.spr.JobDetails">
				<property name="job" value="Trainee Associate" />
				<property name="company" value="Wipro" />
				<property name="location" value="Hyderabad" />
	</bean>
	<bean id="beanVatsal" class="com.java.spr.Person">
		<property name="id" value="1" />
		<property name="name" value="Vatsal" />
		<property name="familyDetails">
			<bean class="com.java.spr.Family">
				<property name="city" value="Pune" />
				<property name="state" value="MH" />
			</bean>
		</property>
	</bean>
	<bean id="beanGanesh" class="com.java.spr.Person">
		<property name="id" value="2" />
		<property name="name" value="Ganesh" />
		<property name="familyDetails">
			<bean class="com.java.spr.Family">
				<property name="city" value="Hyderabad" />
				<property name="state" value="TG" />
			</bean>
		</property>
	</bean>
	<bean id="beanVenkatSai" class="com.java.spr.Person">
		<property name="id" value="2" />
		<property name="name" value="Venkat Sai" />
		<property name="familyDetails">
			<bean class="com.java.spr.Family">
				<property name="city" value="Vizag" />
				<property name="state" value="AP" />
			</bean>
		</property>
	</bean>
</beans>